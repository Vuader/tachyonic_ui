{
    "tachyonic:public": "True",
    "is_root": "$context.roles:Root",
    "is_ops": "$context.roles:Operations",
    "is_admin": "$context.roles:Administrator",
    "is_support": "$context.roles:Support",
    "is_accounts": "$context.roles:AccountManager",
    "is_billing": "$context.roles:Billing",
    "is_customer": "$context.roles:Customer",
    "is_user": "$context.login:True",
    "tachyonic:login": "Rule:is_user",
    "users:admin": "Rule:is_root or Rule:is_ops or Rule:is_admin or Rule:is_support",
    "users:view": "Rule:is_root or Rule:is_ops or Rule:is_admin or Rule:is_support",
    "roles:admin": "Rule:is_root",
    "roles:view": "Rule:is_root",
    "domains:admin": "Rule:is_root",
    "domains:view": "Rule:is_root",
    "tenants:admin": "Rule:is_root or Rule:is_ops or Rule:is_admin or Rule:is_support or Rule:is_billing or Rule:is_accounts or Rule:is_billing",
    "tenants:view": "Rule:is_root or Rule:is_ops or Rule:is_admin or Rule:is_support or Rule:is_billing or Rule:is_accounts or Rule:is_billing",
    "network:admin": "Rule:is_root or Rule:is_ops",
    "network:users": "Rule:is_user",
    "broadband:admin": "Rule:is_root or Rule:is_ops",
    "broadband:users": "Rule:is_user",
    "hosting:admin": "Rule:is_root or Rule:is_ops",
    "hosting:users": "Rule:is_user",
    "cloud:admin": "Rule:is_root or Rule:is_ops",
    "cloud:users": "Rule:is_user",
    "transmission:admin": "Rule:is_root or Rule:is_ops",
    "transmission:users": "Rule:is_user",
    "billing:admin": "Rule:is_root or Rule:is_billing",
    "billing:users": "Rule:is_user"
}
